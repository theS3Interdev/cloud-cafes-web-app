generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Cafe {
  id        String   @id @default(cuid())
  name      String
  city      String
  userId    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
